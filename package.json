{
  "name": "hjemmebehandling-patient-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@kvalitetsit/hjemmebehandling": "^1.4.100",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.11.11",
    "@mui/icons-material": "^5.11.11",
    "@mui/lab": "^5.0.0-alpha.54",
    "@openapitools/openapi-generator-cli": "^2.4.26",
    "eslint-plugin-jsdoc": "^46.4.6",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.3",
    "next": "^12.3.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.4"
  },
  "devDependencies": {
    "@playwright/test": "1.30.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/node": "^20.5.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/react-router-dom": "^5.3.3",
    "http-proxy-middleware": "^2.0.1",
    "react-scripts": "^4.0.3",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "dev": "next dev",
    "build": "NODE_OPTIONS=\"--openssl-legacy-provider\" next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test-watch": "jest --watch",
    "e2e-test": "playwright test",
    "update-visual-snapshot": "playwright test --update-snapshots",
    "preinstall": "rm -rf generated; openapi-generator-cli generate -i resources/bff.json -g typescript-fetch -o generated --additional-properties=typescriptThreePlus=true"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testMatch": [
      "**/?(*.)+(test).[jt]s?(x)"
    ]
  }
}
